<script>
    $(document).ready(function()
    {

        var sc=$(window).scrollTop();
        if (sc>=730)
        {
            $(".navbar-inverse").css({"backgroundColor":"black" , "marginTop":"0"});
            $(".navbar-inverse").css("opacity" , ".6");
        }
        $(window).on("scroll" , function()
        {
            var sc=$(window).scrollTop();
            if (sc>=630)
            {
                $(".navbar-inverse").css({"backgroundColor":"black" , "marginTop":"0"});
                $(".navbar-inverse").css("opacity" , ".6");
            }
            else
            {
                $(".navbar-inverse").css({"backgroundColor":"transparent" , "marginTop":"30px"});
                $(".navbar-inverse").css("opacity" , "1");

            }
        });

        $(".navbar-inverse").mouseenter(function()
        {
            var temp =document.getElementById("mynvigate");
            if (temp.style.opacity <=1)
            {
                this.style.opacity = 1;
            }
        });
        $(".navbar-inverse").mouseleave(function()
        {
            var sc=$(window).scrollTop();
            var temp =document.getElementById("mynvigate");

            if (sc>630)
            {
                this.style.opacity = 0.6;
            }
        });

        $(".navigator").click(function ()
        {
            $(this).addClass("activate").siblings().removeClass("activate");
        });
    });
</script>
